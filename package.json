      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/eslint-parser": "7.22.5",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@babel/preset-env": "7.22.5",
    "gh-pages": "^5.0.0",
    "react-icons": "^4.10.1",
    "shx": "^0.3.4",
    "tailwindcss": "^3.3.3"
  }
